<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.morrow.modules.log.mapper.SysLogErrorMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.morrow.modules.log.entity.SysLogError">
        <id column="id" property="id" />
        <result column="request_uri" property="requestUri" />
        <result column="request_method" property="requestMethod" />
        <result column="request_params" property="requestParams" />
        <result column="user_agent" property="userAgent" />
        <result column="ip" property="ip" />
        <result column="error_info" property="errorInfo" />
        <result column="creator" property="creator" />
        <result column="create_date" property="createDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, request_uri, request_method, request_params, user_agent, ip, error_info, creator, create_date
    </sql>

</mapper>
